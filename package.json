{
  "name": "telos-react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@mdx-js/react": "^1.5.1",
    "@theme-ui/components": "^0.2.50",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^1.2.5",
    "react-scripts": "3.2.0",
    "theme-ui": "^0.2.49"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write ./**/*.tsx"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@blockmatic/dev-scripts": "^1.6.1",
    "@blockmatic/eosio-hooks": "^0.1.5",
    "@blockmatic/eslint-config": "^1.2.0",
    "@blockmatic/eslint-config-react": "^1.2.0",
    "@blockmatic/prettier-config": "^1.0.4",
    "@blockmatic/tsconfig": "^1.2.3",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/theme-ui": "^0.2.5",
    "eslint": "^6.7.1",
    "eslint-plugin-jest": "^23.0.5",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "typescript": "3.7.2"
  }
}
